<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".Activities.BartenderActivity$PlaceholderFragment"
    >
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Bottle Name"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="191dp"
        tools:text="Amount Left" />

    <Button
        android:id="@+id/edit_bottles4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colorButton"
        android:text="Submit Bottle Change"
        android:textColor="@color/whiteText"
        tools:layout_editor_absoluteX="306dp"
        tools:layout_editor_absoluteY="191dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="220dp"
        android:layout_height="44dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        android:text="Change Bottles"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/EditTextName"
        android:layout_width="1196dp"
        android:layout_height="352dp"
        android:background="@drawable/rectangle"
        android:inputType="text"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="301dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Graphs of Inventory"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="266dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Bottle Number"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="84dp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Bottle Name"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="137dp" />


</android.support.constraint.ConstraintLayout>